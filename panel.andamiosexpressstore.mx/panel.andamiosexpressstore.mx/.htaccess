# Impedir que se pueda ver el listado de contenidos de un directorio

Options All -Indexes

# Url Amigables

RewriteEngine On

RewriteRule ^([-a-zA-Z0-9/]+)$ index.php?ruta=$1

RewriteEngine On
RewriteCond %{HTTP_HOST} ^panel.andamiosexpressstore\.mx$ [OR]
RewriteCond %{HTTP_HOST} ^www\.panel.andamiosexpressstore\.mx$
RewriteCond %{HTTPS} !on
RewriteRule ^(.*)$ https://panel.andamiosexpressstore.mx/$1 [R=301,L,NE]
